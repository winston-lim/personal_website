{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:development": "env-cmd -f .env.devsite next build",
    "build:production": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "postinstall": "husky install",
    "prepare": "husky install",
    "commitmsg": "node node_modules/commitplease .git/COMMIT_EDITMSG"
  },
  "commitplease": {
    "limits": {
      "firstLine": "72",
      "otherLine": "80"
    },
    "style": "jquery",
    "component": true,
    "components": [
      "Build",
      "Fix",
      "Develop",
      "Refactor",
      "Delete",
      "Merge",
      "Test",
      "Update"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^6",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.4.0",
    "sass": "^1.54.1"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "commitplease": "^3.2.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "typescript": "4.7.2"
  }
}
